{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "scouting-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@mui/icons-material": "7.3.1",
    "@mui/material": "7.3.1",
    "@mui/system": "7.3.1",
    "@mui/x-data-grid": "8.7.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "body-parser": "^1.20.3",
    "express": "^4.19.2",
    "http-proxy-middleware": "^3.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.0.1",
    "recharts": "^3.1.0",
    "reselect": "^4.1.8",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env PORT=3002 react-scripts start",
    "dev-api": "node scripts/dev-api.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watch",
    "eject": "react-scripts eject",
    "gen:logs": "node scripts/genPitchingManifest.mjs",
    "gen:hlogs": "node scripts/genHittingManifest.mjs",
    "gen:agg": "node scripts/genPitchingAggregates.mjs",
    "cypress:open": "concurrently -k -s first \"cross-env PORT=3002 react-scripts start\" \"wait-on http://localhost:3002 && cypress open\"",
    "cypress:run": "concurrently -k -s first \"cross-env PORT=3002 react-scripts start\" \"wait-on http://localhost:3002 && cypress run\"",
    "audit:arsenals": "node scripts/auditArsenals.mjs"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.14.2",
    "wait-on": "^7.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
